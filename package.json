{
  "name": "nova-demo",
  "version": "0.0.1",
  "description": "A Demo Project",
  "author": "Frank Simon Lechner",
  "keywords": [
    "typescript",
    "react",
    "redux",
    "jest",
    "react-styleguidist",
    "rest"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fsl0110/nova-demo.git"
  },
  "main": "src/index.tsx",
  "private": true,
  "dependencies": {
    "@types/d3": "^5.7.2",
    "@types/helmet": "^0.0.43",
    "@types/react-router-dom": "^4.3.4",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "d3": "^5.9.7",
    "dompurify": "^1.0.11",
    "helmet": "^3.19.0",
    "highcharts": "^7.1.2",
    "highcharts-no-data-to-display": "^0.1.7",
    "highcharts-react-official": "^2.2.2",
    "immer": "^3.1.3",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-swipeable-routes": "^0.6.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "source-map-explorer": "^2.0.1",
    "typescript": "3.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:run": "yarn build && serve ./build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "serve": "serve ./build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "styleguide": "yarn styleguidist server",
    "styleguide:build": "yarn styleguidist build",
    "styleguide:build:run": "yarn styleguidist build && serve ./styleguide"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/dompurify": "^0.0.33",
    "@types/enzyme": "^3.10.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "24.0.15",
    "@types/moxios": "^0.4.8",
    "@types/node": "12.6.2",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools-extension": "^2.13.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "moxios": "^0.4.0",
    "react-docgen-typescript": "^1.12.5",
    "react-styleguidist": "^9.1.11",
    "redux-devtools-extension": "^2.13.8",
    "serve": "^11.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/build/",
      "!<rootDir>/styleguide/",
      "!<rootDir>/src/react-app-env.d.ts",
      "!<rootDir>/src/serviceWorker.ts",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/utils/tests/index.ts",
      "!<rootDir>/src/utils/api/index.ts",
      "!<rootDir>/src/store/index.ts",
      "!<rootDir>/src/pages/index.ts",
      "!<rootDir>/src/components/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
